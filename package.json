{
  "name": "project_tracker",
  "version": "1.0.0",
  "description": "The Project Management Tool Favored by Developers.",
  "main": "src/App.js",
  "scripts": {
    "server": "NODE_ENV=production babel-node src/App.js",
    "start": "nodemon --exec babel-node src/App.js",
    "test": "npm run db-migrate:test && NODE_ENV=test nyc --require @babel/register  mocha ./src/__tests__/*.test.js --timeout 20000 --exit",
    "db-migrate": "sequelize db:migrate --env production",
    "db-migrate:dev": "sequelize db:migrate --env development",
    "db-seeder:dev": "sequelize db:seed:all --env development",
    "db-delete:dev": "sequelize db:migrate:undo:all --env development",
    "db-migrate:test": "sequelize db:migrate:undo:all --env test && sequelize db:migrate --env test",
    "db:reset":  "sequelize db:migrate:undo:all && sequelize db:drop && sequelize db:create && sequelize db:migrate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Emyboy/project_tracker.git"
  },
  "keywords": [],
  "author": "Ifeora Chukwuemeka (iDegin)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Emyboy/project_tracker/issues"
  },
  "homepage": "https://github.com/Emyboy/project_tracker#readme",
  "dependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/node": "^7.8.3",
    "@babel/polyfill": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/register": "^7.8.3",
    "@sendgrid/mail": "^6.5.1",
    "bcrypt": "^4.0.0",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "method-override": "^3.0.0",
    "mocha": "^7.1.0",
    "morgan": "^1.9.1",
    "nodemon": "^2.0.2",
    "nyc": "^15.0.0",
    "pg": "^7.18.2",
    "sequelize": "^5.21.5",
    "sequelize-cli": "^5.5.1",
    "swagger-jsdoc": "^3.5.0",
    "swagger-ui-express": "^4.1.3"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-google": "^0.14.0"
  }
}
